# Basic stuff
topicprefix = <%= mc_topicprefix %>
libdir      = <%= mc_libdir %>
logfile     = <%= mc_logfile %>
loglevel    = <%= mc_loglevel %>
daemonize   = <%= mc_daemonize %>

# Plugins
securityprovider = <%= mc_security_provider_real %>
plugin.psk       = <%= mc_security_psk_real %>

# AES Security Plugin
plugin.aes.serializer = <%= mc_plugin_aes_serializer %>
plugin.aes.send_pubkey = <%= mc_plugin_aes_send_pubkey %>
plugin.aes.learn_pubkeys = <%= mc_plugin_aes_learn_pubkeys %>
plugin.aes.client_cert_dir = <%= mc_plugin_aes_client_cert_dir %>
plugin.aes.server_private = <%= mc_plugin_aes_server_private %>
plugin.aes.server_public = <%= mc_plugin_aes_server_public %>
plugin.aes.enforce_ttl = <%= mc_plugin_aes_enforce_ttl %> 

# Registration
registerinterval = 300
registration = Meta

# Middleware
connector = stomp
plugin.stomp.host     = <%= stomp_server %>
plugin.stomp.port     = <%= stomp_port %>
plugin.stomp.user     = <%= stomp_user %>
plugin.stomp.password = <%= stomp_passwd %>

# NRPE
plugin.nrpe.conf_dir  = <%= nrpe_dir_real %>

# Facts
factsource = <%= fact_source %>
<% if fact_source == 'yaml' -%>
plugin.yaml = <%= yaml_facter_source %>
<% end -%>
